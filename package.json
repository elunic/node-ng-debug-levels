{
  "name": "ng-debug-levels",
  "version": "16.0.0",
  "description": "Provides a LogModule to integrate debug with levels in Angular.",
  "repository": {
    "type": "git",
    "url": "https://github.com/elunic/node-ng-debug-levels.git"
  },
  "author": {
    "name": "elunic AG/William Hefter",
    "email": "wh@elunic.com"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build ng-debug-levels",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "shell:build": "docker-compose -f docker-compose.shell.yml build",
    "shell": "npm run shell:build && docker-compose -f docker-compose.shell.yml run --rm -u 1000 --service-ports --name ng-debug-levels-dev app bash || true && echo Stopping environment... && npm run shell:stop",
    "shell:join": "docker exec -it ng-debug-levels-dev bash || true",
    "shell:stop": "docker-compose -f docker-compose.shell.yml down"
  },
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@elunic/debug-levels": "^0.2.9",
    "debug": "^4.3.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.11",
    "@angular/cli": "^16.2.11",
    "@angular/compiler-cli": "^16.2.0",
    "@types/debug": "^4.1.12",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^16.2.0",
    "typescript": "~5.1.3"
  }
}
