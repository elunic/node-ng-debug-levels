{
  "name": "ng-debug-levels",
  "version": "0.2.2",
  "description": "Provides a LogModule to integrate debug with levels in Angular 9+.",
  "repository": {
    "type": "git",
    "url": "https://github.com/elunic/node-ng-debug-levels.git"
  },
  "author": {
    "name": "elunic AG/William Hefter",
    "email": "wh@elunic.com"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build ng-debug-levels",
    "build:lib:prod": "ng build ng-debug-levels --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "shell:build": "docker-compose -f docker-compose.shell.yml build",
    "shell": "npm run shell:build && docker-compose -f docker-compose.shell.yml run --rm -u 1000 --service-ports --name ng-debug-levels-dev app bash || true && echo Stopping environment... && npm run shell:stop",
    "shell:join": "docker exec -it ng-debug-levels-dev bash || true",
    "shell:stop": "docker-compose -f docker-compose.shell.yml down",
    "check:lib": "ng lint ng-debug-levels --type-check --tslint-config projects/ng-debug-levels/tslint.fix.json --format codeFrame && prettier --check 'projects/ng-debug-levels/**/*.{ts,js,html,css,scss,sass,less}'",
    "fix:lib": "ng lint ng-debug-levels --fix --type-check --tslint-config projects/ng-debug-levels/tslint.fix.json --format codeFrame && prettier --write 'projects/ng-debug-levels/**/*.{ts,js,html,css,scss,sass,less}'"
  },
  "dependencies": {
    "@angular/animations": "~9.1.12",
    "@angular/common": "~9.1.12",
    "@angular/compiler": "~9.1.12",
    "@angular/core": "~9.1.12",
    "@angular/forms": "~9.1.12",
    "@angular/platform-browser": "~9.1.12",
    "@angular/platform-browser-dynamic": "~9.1.12",
    "@angular/router": "~9.1.12",
    "debug": "^4.3.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular-devkit/build-ng-packagr": "~0.901.12",
    "@angular/cli": "~9.1.12",
    "@angular/compiler-cli": "~9.1.12",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@elunic/debug-levels": "^0.2.7",
    "@elunic/ecs-commitlint": "^0.1.1",
    "@elunic/ecs-prettier": "^0.2.1",
    "@elunic/ecs-tsconfig": "^0.1.1",
    "@elunic/ecs-tslint": "^0.1.2",
    "@types/debug": "^4.1.5",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "husky": "^4.3.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "^10.5.1",
    "ng-packagr": "^9.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
